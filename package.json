{
	"name": "today-api.stuysu.org",
	"version": "1.0.0",
	"description": "API for today.stuysu.org",
	"main": "src/index.js",
	"scripts": {
		"start": "node --require esm src/index.js",
		"dev": "nodemon --require esm src/index.js",
		"migrate": "node --require esm node_modules/sequelize-cli/lib/sequelize db:migrate",
		"seedSchedules": "node --require esm src/defaultSchedules.js",
		"build": "npm run migrate"
	},
	"dependencies": {
		"@apollo/server": "^4.3.2",
		"body-parser": "^1.20.1",
		"connect-session-sequelize": "^7.1.5",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"esm": "^3.2.25",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"google-auth-library": "^8.7.0",
		"graphql": "^16.6.0",
		"graphql-scalars": "^1.20.1",
		"graphql-validation-complexity": "^0.4.2",
		"jsonwebtoken": "^9.0.0",
		"mysql2": "^3.0.1",
		"sequelize": "^6.28.0",
		"sequelize-cli": "^6.6.0"
	},
	"devDependencies": {
		"nodemon": "^2.0.20",
		"sqlite3": "^5.1.4"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"arrowParens": "avoid",
		"trailingComma": "none",
		"semi": true,
		"singleQuote": true,
		"printWidth": 80,
		"endOfLine": "lf"
	}
}
